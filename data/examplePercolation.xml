<?xml version="1.0"?>
<CellSociety game="PERCOLATION">
    <Title>Cell Society - Fire Example</Title>
    <Authors>
        <Author>Leo Cao</Author>
        <Author>Nolan Gelinas</Author>
        <Author>Zack Schrage</Author>
    </Authors>
    <Description>Basic initially empty Fire game. Playing the simulation will generate trees and fires.</Description>
    <Dimentions>
        <Width>25</Width>
        <Height>25</Height>
    </Dimentions>
    <InitialCellState>
        <Cell x="5" y="5" type="WATER"/>
        <Cell x="5" y="6" type="BLOCK"/>
        <Cell x="5" y="7" type="BLOCK"/>
        <Cell x="5" y="8" type="BLOCK"/>
        <Cell x="5" y="9" type="BLOCK"/>
    </InitialCellState>
    <Parameters>
        <Speed>1</Speed>
    </Parameters>
    <ColorSceme>
        <Color type="EMPTY">#000000</Color>
        <Color type="WATER">#0000ff</Color>
        <Color type="BLOCK">#59ff33</Color>
    </ColorSceme>
</CellSociety>

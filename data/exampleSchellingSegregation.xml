<?xml version="1.0"?>
<CellSociety game="SCHELLSEG">
    <Title>Cell Society - Schelling Segregation Example</Title>
    <Authors>
        <Author>Leo Cao</Author>
        <Author>Nolan Gelinas</Author>
        <Author>Zack Schrage</Author>
    </Authors>
    <Description>Basic Schelling Segregation Game</Description>
    <Dimentions>
        <Width>50</Width>
        <Height>50</Height>
    </Dimentions>
    <InitialCellState>
        <Cell x="5" y="5" type="A"/>
        <Cell x="6" y="5" type="B"/>
        <Cell x="7" y="5" type="B"/>
        <Cell x="10" y="10" type="B"/>
        <Cell x="11" y="10" type="A"/>
        <Cell x="12" y="10" type="A"/>
        <Cell x="12" y="9" type="B"/>
        <Cell x="11" y="8" type="A"/>
    </InitialCellState>
    <Parameters>
        <Speed>1</Speed>
        <fIdeal>0.5</fIdeal>
        <NeighborhoodType>SQUARE_MOORE</NeighborhoodType>
    </Parameters>
    <ColorSceme>
        <Color type="A">#000000</Color>
        <Color type="B">#ffff00</Color>
    </ColorSceme>
</CellSociety>

<?xml version="1.0"?>
<CellSociety game="FIRE">
    <Title>Cell Society - Fire Example</Title>
    <Authors>
        <Author>Leo Cao</Author>
        <Author>Nolan Gelinas</Author>
        <Author>Zack Schrage</Author>
    </Authors>
    <Description>Basic initially empty Fire game. Playing the simulation will generate trees and fires.</Description>
    <Dimentions>
        <Width>50</Width>
        <Height>50</Height>
    </Dimentions>
    <InitialCellState>
    </InitialCellState>
    <Parameters>
        <Speed>3</Speed>
        <TreeCombustProbability>0.2</TreeCombustProbability>
        <TreeGrowthProbability>0.3</TreeGrowthProbability>
        <NeighborhoodType>SQUARE_NEUMANN</NeighborhoodType>
    </Parameters>
    <ColorSceme>
        <Color type="EMPTY">white</Color>
        <Color type="BURNING">orange</Color>
        <Color type="TREE">brown</Color>
    </ColorSceme>
</CellSociety>
